{
    "hosting": {
      "rewrites": [
        {
          "source": "/api/**",
          "run": {
            "serviceId": "backend",
            "region": "northamerica-northeast1"
          }
        },
        {
          "source": "**",
          "run": {
            "serviceId": "frontend",
            "region": "northamerica-northeast1"
          }
        }
      ]
    }
  }