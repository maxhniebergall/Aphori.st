    "eject": "react-scripts eject",
    "analyze": "yarn build --stats && webpack-bundle-analyzer build/bundle-stats.json",
    "typecheck": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-use-before-define": [
        "error",
        {
          "functions": false,
          "classes": false
